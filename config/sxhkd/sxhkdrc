super + control + r
    pkill -USR1 -x sxhkd

super + p
   launchers rofi -theme ~/.config/rofi/themes/launcher.rasi

super + Return
    st -n "Simple Terminal" -T "Simple Terminal"

super + control + Return
    st_tabbed

super + e
    pcmanfm

super + shift + e
    st -n nvim-ide -g 164x52 -e nvim

super + shift + Delete
    powermenu

XF86MonBrightnessDown
    bright -d 2 ; kill -58 $(pidof dwmblocks)

XF86MonBrightnessUp
    bright -i 2 ; kill -58 $(pidof dwmblocks)

XF86AudioMute
    vol -t ; kill -59 $(pidof dwmblocks)

XF86AudioLowerVolume
    vol -d 2% ; kill -59 $(pidof dwmblocks)

XF86AudioRaiseVolume
    vol -i 2% ; kill -59 $(pidof dwmblocks)

XF86AudioMicMute
    mic -t

control + Print
    maim | tee ~/images/$(date +%s).png | xclip -selection clipboard -t image/png

shift + Print
    maim -i $(xdotool getactivewindow) | xclip -selection clipboard -t image/png

