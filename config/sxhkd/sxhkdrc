super + control + r
    pkill -USR1 -x sxhkd

super + p
   applauncher rofi -theme ~/.config/rofi/themes/launcher.rasi

super + Return
    stwrapper

super + control + Return
    stwrapper -t

super + e
    pcmanfm

super + shift + e
    st -n nvim-ide -g 164x52 -e nvim

super + shift + Delete
    powermenu

XF86MonBrightnessDown
    bright -d 2

XF86MonBrightnessUp
    bright -i 2

XF86AudioMute
    vol -t

XF86AudioLowerVolume
    vol -d 2%

XF86AudioRaiseVolume
    vol -i 2%

XF86AudioMicMute
    mic -t

control + Print
    maim | tee ~/images/$(date +%s).png | xclip -selection clipboard -t image/png

shift + Print
    maim -i $(xdotool getactivewindow) | xclip -selection clipboard -t image/png

